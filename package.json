{
  "name": "shapey",
  "version": "1.3.1",
  "description": "A simple syntax for remapping objects, inspired by several of Ramda's spec based functions",
  "main": "lib/index.js",
  "types": "types.d.ts",
  "private": true,
  "watch": {
    "docs": "{assets,src}/*"
  },
  "scripts": {
    "predist": "npm run prepare",
    "dist": "npm publish build",
    "docs": "jsdoc --configure jsdoc.json",
    "docs:server": "live-server --open=docs --watch=assets,docs --port=5000",
    "start": "npm run test:watch",
    "clean": "rimraf dist es cjs coverage",
    "prestart:docs": "npm run docs",
    "start:docs": "concurrently \"npm-watch\" \"npm run docs:server\"",
    "test:watch": "nodemon test/index.js",
    "test:coverage": "nyc tape --require=@babel/register test/index.js | tap-nyc",
    "test": "tape --require=@babel/register test/index.js",
    "lint": "eslint lib test",
    "prepare": "npm run clean && npm run build",
    "build": "node scripts/build-package/run.js"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": "github:arizonatribe/shapey",
  "homepage": "https://github.com/arizonatribe/shapey#readme",
  "bugs": "https://github.com/arizonatribe/shapey/issues",
  "keywords": [
    "mapping",
    "spec",
    "ramda",
    "nodejs",
    "javascript",
    "evolve",
    "enhancer",
    "helper",
    "utils",
    "functional",
    "programming"
  ],
  "license": "Apache-2.0",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "dependencies": {
    "vanillas": "^1.0.17"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chalk": "2.4.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-flowtype": "3.4.1",
    "eslint-plugin-import": "2.14.0",
    "faucet": "0.0.1",
    "fs-extra": "7.0.0",
    "glob": "7.1.1",
    "jsdoc": "^3.6.3",
    "jsdoc-export-default-interop": "^0.3.1",
    "nodemon": "1.18.10",
    "ramda": "0.26.1",
    "rimraf": "2.6.3",
    "rollup": "~2.38.0",
    "rollup-plugin-terser": "^7.0.0",
    "tape": "4.9.1",
    "tui-jsdoc-template": "1.2.2"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "babylon",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
