{
  "name": "shapey",
  "version": "1.2.2",
  "description": "A simple syntax for remapping objects, inspired by several of Ramda's spec based functions",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "debug": "nodemon test/index.js --watch lib --watch test --inspect --quiet --exec 'clear && npm run test'",
    "start": "nodemon test/index.js --watch lib --watch test --quiet --exec 'clear && npm run test'",
    "clean": "rimraf dist es coverage",
    "lint": "eslint lib test",
    "test": "cross-env BABEL_ENV=commonjs babel-node test | faucet",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel lib --out-dir cjs",
    "build:es": "cross-env BABEL_ENV=es babel lib --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -i lib/index.js -o dist/shapey.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -i lib/index.js -o dist/shapey.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/shapey.git"
  },
  "keywords": [
    "mapping",
    "spec",
    "ramda",
    "nodejs",
    "javascript",
    "evolve",
    "enhancer",
    "helper",
    "utils",
    "functional",
    "programming"
  ],
  "license": "Apache-2.0",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "dependencies": {
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-ramda": "1.4.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "babel-tape-runner": "2.0.1",
    "cross-env": "5.1.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.2.0",
    "faucet": "0.0.1",
    "glob": "7.1.1",
    "jsdoc": "3.5.5",
    "nodemon": "1.11.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "tape": "4.8.0",
    "uglify-es": "3.3.9"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "babylon",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
